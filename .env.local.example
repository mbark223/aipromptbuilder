# Luma AI API Key
NEXT_PUBLIC_LUMA_AI_API_KEY=your-luma-api-key

# Firebase Configuration - Local Development
# Option 1: Use Firebase Emulator (no real credentials needed)
NEXT_PUBLIC_FIREBASE_API_KEY=demo-api-key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=demo-project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=demo-project
NEXT_PUBLIC_FIREBASE_APP_ID=demo-app-id

# For Firebase Admin SDK with emulator
FIREBASE_PROJECT_ID=demo-project
FIREBASE_CLIENT_EMAIL=demo@demo-project.iam.gserviceaccount.com
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\ndemo-key\n-----END PRIVATE KEY-----"

# Firebase Emulator Settings
FIREBASE_AUTH_EMULATOR_HOST=localhost:9099
FIRESTORE_EMULATOR_HOST=localhost:8080

# Other API keys
REPLICATE_API_TOKEN=
OPENROUTER_API_KEY=
OPENAI_API_KEY=